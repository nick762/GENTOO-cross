    <schema>
      <key>/schemas/apps/metacity/general/resize_with_right_button</key>
      <applyto>/apps/metacity/general/resize_with_right_button</applyto>
      <owner>metacity</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
         <short>Whether to resize with the right button</short>
         <long>
           Set this to true to resize with the right button and show a menu
           with the middle button while holding down the key given in
           "mouse_button_modifier"; set it to false to make it work the
           opposite way around.
         </long>
      </locale>

      <locale name="as">
         <short>সোঁফালৰ বুটাম প্ৰয়োগ কৰি মাপ পৰিবৰ্তন কৰা হ&apos;ব নে নহয়</short>
         <long>&quot;mouse_button_modifier&quot; ত চিহ্নিত চাবি টিপ ধৰি, সোঁফালৰ বুটাম সহযোগে মাপ  পৰিবৰ্তন আৰু মাজৰ বুটাম সহযোগে মেনু প্ৰদৰ্শন কৰাৰ বাবে এই মান true ধাৰ্য কৰক; বিপৰীত বৈশিষ্ট্য সহযোগে কাৰ্যকৰী কৰাৰ বাবে মান false ধাৰ্য কৰক ।</long>
      </locale>

      <locale name="be@latin">
